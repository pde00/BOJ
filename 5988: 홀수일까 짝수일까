#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
using namespace std;


void isEven(string num) {
	if (num[num.size() - 1] == '0' || num[num.size() - 1] == '2' || num[num.size() - 1] == '4' || num[num.size() - 1] == '6' || num[num.size() - 1] == '8') {
		cout << "even" << '\n';
		return;
	}
	cout << "odd" << '\n';
}

int main() {
	int N;
	string num;
	cin >> N;
	for (int i = 0; i < N; i++) {
		cin >> num;
		isEven(num);
	}


}
