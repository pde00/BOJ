#include <iostream>
using namespace std;

int N, v, tpN, ar[201];

int main(void){
    cin.tie(NULL);
    ios_base::sync_with_stdio(false);

    cin >> N;
    for(int i = 0; i < N; i++){
        cin >> tpN;
        ++ar[tpN + 100];
    }
    cin >> v;
    cout << ar[v + 100] << '\n';

    return 0;
}
