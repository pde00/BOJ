#include <iostream>
#include <algorithm>
using namespace std;

int score[1000];

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int N,max;
	double sum = 0.0;
	cin >> N;
	cout.precision(4);

	for (int i = 0; i < N; i++){cin >> score[i];}
	
	sort(score, score + N);
	max = score[N - 1];

	for (int i = 0; i < N; i++) {
		sum += (double)score[i] / (double)max * 100.0;  // print! 
	}
	cout<< sum / (double)N << '\n';
}


